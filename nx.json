{
  "extends": "@nrwl/workspace/presets/core.json",
  "npmScope": "easy",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": ""
      }
    }
  },
  "workspaceLayout": {
    "appsDir": "packages",
    "libsDir": "packages/libs"
  }
}
